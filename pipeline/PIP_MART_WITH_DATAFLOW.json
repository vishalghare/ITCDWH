				}
			}
		],
		"parameters": {
			"P_PIPELINE_NAME": {
				"type": "string"
			}
		},
		"variables": {
			"V_PERIOD_START": {
				"type": "String"
			},
			"V_PERIOD_END": {
				"type": "String"
			},
			"V_SNAP_DATE": {
				"type": "String"
			},
			"V_BATCH_NBR": {
				"type": "String"
			}
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
