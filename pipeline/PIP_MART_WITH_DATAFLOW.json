					}
				}
			},
			{
				"name": "Set PERIOD END",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "GetPipelineRunInfo",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "V_PERIOD_END",
					"value": {
						"value": "@activity('GetPipelineRunInfo').output.firstRow.PERIOD_END",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Start the Pipeline",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
